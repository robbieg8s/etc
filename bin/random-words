#!/bin/zsh -ef

# Emit some random words
# Word list from https://github.com/bitcoin/bips/blob/master/bip-0039/english.txt
# Data below product via
# curl --silent https://raw.githubusercontent.com/bitcoin/bips/master/bip-0039/english.txt | lzma -9 | base64 --break 80

count=${1:-1}

() {
  local words=( "${(@f)$(cat "${1}")}" )
  for i in {1..${count}}
  do
    echo $words[$(( 1 + (RANDOM % ${#words}) ))]
  done
} <( { base64 -D | lzma --decompress ; } <<'END_BIP39'
